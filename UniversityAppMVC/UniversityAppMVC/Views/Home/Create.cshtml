@model UniversityAppMVC.Models.Student
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm())
{

    @Html.LabelFor(model => model.StdId)
    @Html.TextBoxFor(model => model.StdId)
    <br />



    @Html.LabelFor(model => model.StdName)
    @Html.TextBoxFor(model => model.StdName)
    <br />



    @Html.LabelFor(model => model.StdAge)
    @Html.TextBoxFor(model => model.StdAge)
    <br />



    @Html.LabelFor(model => model.StdCity)
    @Html.TextBoxFor(model => model.StdCity)
    <br />



   


    <input type="Submit" name="submit" />





}
